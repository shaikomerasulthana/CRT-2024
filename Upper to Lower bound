#include <stdio.h>
#include<string.h>

int main() {
    char str1[50];
    scanf("%[^\n]s",str1);
    int i;
    for(i=0;str1[i]!='\0';i++)
    {
        if(str1[i]>=65 && str1[i]<=90)
        {
        str1[i]=str1[i]+32;
        }
        else if(str1[i]>=97 && str1[i]<=122)
        {
        str1[i]=str1[i]-32;
        }
    }
    printf("%s",str1);
}
