#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

     char a;
    scanf("%c",&a);
    if(a>='A'&&a<='Z')
    {
        printf("Upper");
    }
    else if(a>'a'&&a<='z')
    {
        printf("Lower");
    }
    else if(a>='0'&&a<='9')
    {
        printf("Number");
    }
    else
    {
        printf("Symbol");
    }
    return 0;
}
