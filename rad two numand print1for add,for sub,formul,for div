#include <stdio.h>

int main() {
   int a,b;
   scanf("%d%d",&a,&b);
   int ch;
   scanf("%d",&ch);
   switch(ch)
   {
       case1:printf("%d",a+b);
       break;
       case2:printf("%d",a-b);
       break;
       case3:printf("%d",a*b);
       break;
       case4:printf("%d",a/b);
       break;
       default:
       printf("invalid choice");
   }
}
