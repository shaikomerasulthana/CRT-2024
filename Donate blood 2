#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int age;
    int weight;
    float hemoglobin;
    scanf("%d\n %d\n %f",&age,&weight,&hemoglobin);
        if(age>=18 && weight>=45 && hemoglobin>=5.5)
            printf("He or She able to donate blood");
    else
    {
        printf("He or She unable to donate blood");
    } 
    return 0;
}
