#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d",&n);
    int a[n],i,m;
    for(i=0;i<n;i++)
    {
scanf("%d",&a[i]);
    }
    m=a[0];
    for(i=1;i<n;i++){
        if(m<a[i])
            m=a[i];
    }
    printf("%d",m);
    return 0;
}
