#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
void pattern(int n)
{
    for(int i=0;i<n;i++)
    {
    for(int j=0;j<i;j++)
    {
printf(" ");
    }
for(int k=0;k<n-i;k++)
{
    printf("*");
}
        printf("\n");
    }
    }
int main()
{
    int n;
    scanf("%d",&n);
    pattern(n);
    printf("\n");
    return 0;
}
